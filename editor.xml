<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_INTENTION_PREVIEW" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JAVA.microservices.url.path.inlay.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="microservices.url.path.inlay.hints" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="11db71" />
    </pattern>
    <pattern pattern="TODO:\b.*" />
    <pattern pattern="(// TODO \(khorvat\):.*)" />
    <filter name="ToDos">
      <pattern index="3" />
    </filter>
  </component>
</application>